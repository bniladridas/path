{
  "version": 2,
  "routes": [
    {
      "src": "/terms",
      "dest": "/app.py"
    },
    {
      "src": "/privacy",
      "dest": "/app.py"
    },
    {
      "src": "/updates",
      "dest": "/app.py"
    },
    {
      "src": "/verify",
      "dest": "/app.py"
    },
    {
      "src": "/bypass-verification",
      "dest": "/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}
