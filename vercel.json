{
  "builds": [
    {
      "src": "api/**.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/requirements.txt",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index"
    }
  ]
}
