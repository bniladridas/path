{
  "buildCommand": "python scripts/prepare-deployment.py vercel",
  "builds": [
    {
      "src": "path/**.py",
      "use": "@vercel/python"
    },
    {
      "src": "path/requirements.txt",
      "use": "@vercel/python"
    },
    {
      "src": "path/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/path/index"
    }
  ]
}
