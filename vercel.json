{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ],
  "build": {
    "env": {
      "PYTHONPATH": "/var/task"
    },
    "installCommand": "pip install -r requirements.txt"
  },
  "env": {
    "PYTHONPATH": "."
  }
}
