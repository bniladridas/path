{
  "buildCommand": "python scripts/prepare-deployment.py vercel",
  "functions": {
    "path/index.py": {
      "runtime": "@vercel/python"
    }
  },
  "builds": [
    {
      "src": "path/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/path/index"
    }
  ]
}
